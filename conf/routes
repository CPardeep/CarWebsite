GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
GET     /                           controllers.GarageController.index()
GET     /addCar                     controllers.GarageController.displayCarForm()
POST    /create                     controllers.GarageController.addCar()
GET     /myCars                     controllers.GarageController.listCar()
PUT     /update/:id                 controllers.GarageController.updateCar(id: reactivemongo.bson.BSONObjectID)