GET     /assets/*file               controllers.Assets.versioned(path="/public", file: Asset)
GET     /                           controllers.HomeController.index()
GET     /carFormPage                controllers.GarageController.displayCarForm()
POST    /create                     controllers.GarageController.addCar()
GET     /getAll                     controllers.GarageController.listCar()
PUT     /update/:id                 controllers.GarageController.updateCar(id: reactivemongo.bson.BSONObjectID)